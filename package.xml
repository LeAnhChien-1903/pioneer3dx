<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>pioneer3dx</name>
  <version>0.0.0</version>
  <description>Slam algorithm on pioneer-3dx platform in webots robot simulations</description>
  <maintainer email="leanhchien1903@gmail.com">leanhchien</maintainer>
  <license>Apache License 2.0</license>

  <depend>rclpy</depend>
  <depend>geometry_msgs</depend>
  <depend>webots_ros2_driver</depend>
  <depend>sensor_msgs</depend>
  <depend>tf2_ros</depend>
  <depend>joint_state_publisher</depend>
  <depend>robot_state_publisher</depend>
  <depend>rviz</depend>
  <depend>nav_msgs</depend>
  <depend>robot_localization</depend>


  
  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
